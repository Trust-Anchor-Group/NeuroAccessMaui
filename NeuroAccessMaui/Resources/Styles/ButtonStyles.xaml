	<Style x:Key="PrimaryButtonActiveBorder"
		   TargetType="Border"
		   BaseResourceKey="ButtonBorderBase">
		<Setter Property="BackgroundColor"
				Value="{AppThemeBinding Light={DynamicResource ButtonAccessPrimarybgWLLight}, Dark={DynamicResource ButtonAccessPrimarybgWLDark}}" />
