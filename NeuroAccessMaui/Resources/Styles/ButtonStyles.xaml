				Value="Center" />
		<Setter Property="StrokeShape"
				Value="RoundRectangle 8" />
		<Setter Property="HeightRequest"
				Value="40" />
		<Setter Property="WidthRequest"
				Value="40" />
		<Setter Property="Shadow">
			<Setter.Value>
				<Shadow Brush="{AppThemeBinding Light={DynamicResource SurfaceShadowColorWLLight}, Dark={DynamicResource SurfaceShadowColorWLDark}}"
						Offset="0,4" />
