		<Setter Property="StrokeMiterLimit" Value="10" />
		<Setter Property="StrokeThickness" Value="{StaticResource ThinStrokeThickness}" />
		<Setter Property="Fill" Value="{AppThemeBinding Light={StaticResource EnabledFilledButtonForegroundLight}, Dark={StaticResource EnabledFilledButtonForegroundDark}}"></Setter>
