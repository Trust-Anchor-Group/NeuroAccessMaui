<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<!-- <TargetFrameworks>$(TargetFrameworks);net10.0-maccatalyst</TargetFrameworks> -->
		<!-- <TargetFrameworks Condition="$([MSBuild]::IsOSPlatform('windows'))">$(TargetFrameworks);net10.0-windows10.0.19041.0</TargetFrameworks> -->
		<!-- Uncomment to also build the tizen app. You will need to install tizen by following this: https://github.com/Samsung/Tizen.NET -->
		<!-- <TargetFrameworks>$(TargetFrameworks);net10.0-tizen</TargetFrameworks> -->
		<OutputType>Exe</OutputType>
		<RootNamespace>NeuroAccessMaui</RootNamespace>
		<UseMaui>true</UseMaui>
		<SingleProject>true</SingleProject>
		<ImplicitUsings>enable</ImplicitUsings>

		<!-- Display name -->
		<ApplicationTitle>Neuro-Access</ApplicationTitle>

		<!-- App Identifier -->
		<ApplicationId Condition="$(TargetFramework) == 'net10.0-android'">com.tag.NeuroAccess</ApplicationId>
		<ApplicationId Condition="$(TargetFramework) != 'net10.0-android'">com.trustanchorgroup.NeuroAccess</ApplicationId>
		<ApplicationIdGuid>2a4dd0f3-35d6-48a3-a76d-1fa94e567eee</ApplicationIdGuid>

		<!-- Versions -->
		<ApplicationDisplayVersion>2.8.0</ApplicationDisplayVersion>
		<Version>2.8.0</Version>
		<ApplicationVersion>712</ApplicationVersion>

		<NeutralLanguage>en</NeutralLanguage>


		<SupportedOSPlatformVersion Condition="$([MSBuild]::GetTargetPlatformIdentifier('$(TargetFramework)')) == 'ios'">15.0</SupportedOSPlatformVersion>
		<SupportedOSPlatformVersion Condition="$([MSBuild]::GetTargetPlatformIdentifier('$(TargetFramework)')) == 'maccatalyst'">13.1</SupportedOSPlatformVersion>
		<SupportedOSPlatformVersion Condition="$([MSBuild]::GetTargetPlatformIdentifier('$(TargetFramework)')) == 'android'">23.0</SupportedOSPlatformVersion>
		<SupportedOSPlatformVersion Condition="$([MSBuild]::GetTargetPlatformIdentifier('$(TargetFramework)')) == 'windows'">10.0.17763.0</SupportedOSPlatformVersion>
		<TargetPlatformMinVersion Condition="$([MSBuild]::GetTargetPlatformIdentifier('$(TargetFramework)')) == 'windows'">10.0.17763.0</TargetPlatformMinVersion>
		<SupportedOSPlatformVersion Condition="$([MSBuild]::GetTargetPlatformIdentifier('$(TargetFramework)')) == 'tizen'">6.5</SupportedOSPlatformVersion>
		<TargetPlatformMinVersion Condition="$([MSBuild]::GetTargetPlatformIdentifier('$(TargetFramework)')) == 'ios'">15.0</TargetPlatformMinVersion>
		<PublishAot>False</PublishAot>
		<TrimMode>partial</TrimMode>
		<Title>Neuro-Access</Title>
		<Company>Trust Anchor Group AB</Company>
		<Product>Neuro-Access</Product>
		<Description>Maui App giving users access to Neuro-services, such as quick-login, digital identities, smart contracts, Neuro-Feature tokens and eDaler.</Description>
		<Copyright>Copyright Â© Trust Anchor Group 2018-2025. All rights reserved.</Copyright>
		<PackageProjectUrl></PackageProjectUrl>
		<RepositoryUrl>https://github.com/Trust-Anchor-Group/NeuroAccessMaui</RepositoryUrl>
		<MauiEnableXamlCBindingWithSourceCompilation>true</MauiEnableXamlCBindingWithSourceCompilation>
	</PropertyGroup>

	<PropertyGroup Condition="'$(Configuration)'=='Debug'">
		<DebugSymbols>true</DebugSymbols>
	</PropertyGroup>

	<!-- If on Mac, add iOS as well -->
	<PropertyGroup Condition="'$(OS)' != 'Windows_NT'">
		<TargetFrameworks>net10.0-ios;net10.0-android</TargetFrameworks>
	</PropertyGroup>

	<PropertyGroup Condition="$([MSBuild]::IsOSPlatform('windows'))">
		<!-- On Windows build Android + Windows (WinUI) -->
		<TargetFrameworks>net10.0-android;net10.0-windows10.0.19041.0</TargetFrameworks>

		<WindowsPackageType>None</WindowsPackageType>

	</PropertyGroup>

	<PropertyGroup Condition="'$(TargetFramework)'=='net10.0-ios'">
		<UseInterpreter>True</UseInterpreter>
		<MtouchLink>SdkOnly</MtouchLink>
		<NoSymbolStrip>True</NoSymbolStrip>
		<MtouchExtraArgs>--optimize:-static-block-to-delegate-lookup</MtouchExtraArgs>
	</PropertyGroup>
	<ItemGroup Condition="'$(TargetFramework)'=='net10.0-ios'">
		<TrimmerRootAssembly Include="CommunityToolkit.Maui" RootMode="All" />
	</ItemGroup>

	<PropertyGroup Condition="'$(Configuration)|$(TargetFramework)|$(Platform)'=='Release|net10.0-ios|AnyCPU'">
		<DebugSymbols>False</DebugSymbols>
		<RunAOTCompilation>True</RunAOTCompilation>
		<UseInterpreter>False</UseInterpreter>
	</PropertyGroup>

	<PropertyGroup Condition="'$(Configuration)|$(TargetFramework)|$(Platform)'=='Debug|net10.0-ios|AnyCPU'">
